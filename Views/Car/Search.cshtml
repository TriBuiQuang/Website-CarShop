@{
    ViewBag.Title = "Search";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<div id="gallery" class="gallery full-gallery de-gallery pf_full_width wow fadeInUp" data-wow-delay=".3s">

    @foreach (var xe in Model)
            {   <!-- gallery item -->


        <div class="item @xe.MaNSX">
            <div class="picframe" style="width:350px;height:200px">
                <a class="simple-ajax-popup-align-top" href="~/Car/Details1/@xe.Maxe">
                    <span class="overlay">
                        <span class="pf_text">
                            <span class="project-name">@xe.Tenxe</span>
                        </span>
                    </span>
                </a>
                <img src="~/Hinhsanpham/@xe.Anh" alt="@xe.Tenxe" style="width:350px;height:200px" />
            </div>
        </div>
                    <!-- close gallery item -->
    }
</div>