@model IEnumerable<Car.Models.SanPham>
@{
    ViewBag.Title = "Details";
    Layout = null;
}

<div id="gallery" class="gallery full-gallery de-gallery pf_full_width wow fadeInUp" data-wow-delay=".3s">

    @foreach (var sp in Model)
            {   <!-- gallery item -->
        <div class="item @sp.MaSP">
            <div class="picframe" style="width:350px;height:200px">
                <a class="simple-ajax-popup-align-top" href="~/Car/Details1/@sp.MaSP">
                    <span class="overlay">
                        <span class="pf_text">
                            <span class="project-name">@sp.TenSP</span>
                        </span>
                    </span>
                </a>
                <img src="~/Hinhsanpham/@sp.AnhBia" alt="@sp.TenSP" style="width:350px;height:200px" />
            </div>
        </div>
                    <!-- close gallery item -->
    }
</div>



