                        </h1>
                        <!-- logo close -->
                        <!-- small button begin -->
                        <span id="menu-btn"></span>
                        <!-- small button close -->
                        <!-- mainmenu begin -->
                        <nav>
                            <ul id="mainmenu">
                                <li>
                                    @*<a href="">Trang Chủ</a>*@
                                    <a href="@Url.Action("Index", "Car")">Trang chủ</a>
                                </li>
                                
