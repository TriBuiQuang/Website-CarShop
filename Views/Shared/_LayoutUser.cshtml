                                    <ul>
                                        @*<li><a href="">Bảo Dưỡng</a></li>*@
                                        <li><a href="@Url.Action("Index3", "Car")">Phụ Tùng & Phụ Kiện </a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!-- mainmenu close -->
                    <div class="row">
                        <div class="col-md-12" style="text-align:right">
                            @if (Session["TaiKhoan"] == null)
                            {
                                <a class="btn-primary btn-sm" href=@Url.Action("Dangnhap","Nguoidung")>Đăng nhập</a>
                                <a class="btn-primary btn-sm" href=@Url.Action("Dangky","Nguoidung")>Đăng Ký</a>
                            }
                            else
                            {

                                <span style="color:red">Xin Chào <a href="@Url.Action("TTNguoiDung","Nguoidung",new { id=Session["MaKH"]})" style="color:red">@Session["TenKH"]</a></span>
                                <a class="btn-primary btn-sm" href=@Url.Action("Dangxuat","Nguoidung")>Đăng Xuất</a>
                            }
                            @Html.Action("GioHangpartial", "GioHang")
                        </div>
                        <div class="col-md-12" style="text-align:center">
                            @using (Html.BeginForm("Search", "Car", FormMethod.Get, new { @class = "navbar-form navbar-right", role = "search", id = "search-form" }))
                            {
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Tìm kiếm" name="SearchTerm" value="@ViewBag.SearchTerm" id="search-box">
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-default"><i class="fas fa-search"></i></button>
                                </div>
                            }
                        </div>
                    </div>
                </div>
