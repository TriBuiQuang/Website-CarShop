            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <!-- logo begin -->
                        <h1 id="logo">
                            <a href="@Url.Action("Index", "Car")">
                                <img class="logo" src="~/images/logo.jpg" height="75px" alt="">
